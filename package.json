{
  "name": "lit-generate",
  "version": "0.0.2",
  "description": "CLI to build Lit Web Components and tools.",
  "author": {
    "name": "Jake Strawn",
    "url": "https://github.com/himerus"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "bin": {
    "lit-generate": "./bin/run"
  },
  "homepage": "https://github.com/inlinejs/lit-generate",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/inlinejs/lit-generate.git"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@oclif/core": "^1.18.0",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.1.2"
  },
  "devDependencies": {
    "@oclif/test": "^2.2.2",
    "@types/chai": "^4",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.65",
    "chai": "^4",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.4",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.3.1",
    "typescript": "^4.8.4"
  },
  "oclif": {
    "bin": "lit-generate",
    "dirname": "lit-generate",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "package": "npm publish"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "bugs": "https://github.com/inlinejs/lit-generate/issues",
  "keywords": [
    "oclif",
    "lit-generate",
    "lit",
    "web-components",
    "lit-element",
    "lit-html",
    "outline",
    "inline",
    "inlinejs",
    "web components"
  ],
  "types": "dist/index.d.ts"
}
